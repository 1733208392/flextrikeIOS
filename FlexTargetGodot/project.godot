[dotnet]

project/assembly_name="GODotTetris"

[editor_plugins]

enabled=PackedStringArray("res://addons/onscreenkeyboard/plugin.cfg")

[gui]

theme/dynamic_fonts/use_oversampling=true

[internationalization]

locale/translations=PackedStringArray("res://translations/translations.en.translation", "res://translations/translations.zh_CN.translation", "res://translations/translations.ja.translation", "res://translations/translations.zh_TW.translation")

[rendering]

